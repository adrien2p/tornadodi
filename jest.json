{
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "json"
  ],
  "transform": {
    "^.+\\.ts?$": "ts-jest"
  },
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "roots": ["src"],
  "collectCoverageFrom" : [
    "src/**/*.{js,jsx,tsx,ts}",
    "!dist/**",
    "!**/node_modules/**"
  ],
  "coverageReporters": ["json", "lcov"]
}